ection(db, \"usuarios\"), {\r\n        nombre: this.usuario.nombre,\r\n        correo: this.usuario.correo\r\n      })\r\n        .then(() => {\r\n          router.go('/')\r\n          console.log(\"Documento añadido\");\r\n        })\r\n        .catch(function(error) {\r\n          console.error(\"Error al añadir el documento: \", error);\r\n        });\r\n    },\r\n    // DELETE / ELIMINAR / BORRAR\r\n    async eliminarDato (id){\r\n      await deleteDoc(doc(db, \"usuarios\", id ));\r\n      router.go('/')\r\n    },\r\n  },\r\n  mounted() {\r\n    this.obtenerDatos();\r\n  },\r\n}\r\n</script>\r\n"],"mappings":";AAmDA,OAAOA,MAAK,MAAO,yBAAwB;AAC3C,SAASC,UAAU,EAAEC,OAAO,EAAEC,MAAM,EAAEC,SAAS,EAAEC,GAAE,QAAS,yBAAyB;AACrF,SAASC,EAAC,QAAS,SAAS;AAC5B,OAAOC,MAAK,MAAO,iBAAgB;AAEnC,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE;IACVT;EACF,CAAC;EACDU,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE;QACTC,MAAM,EAAE,EAAE;QACVC,MAAM,EAAE;MACV;IACA;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAWA,CAAA,EAAK;MACpB,MAAMC,aAAY,GAAI,MAAMf,OAAO,CAACD,UAAU,CAACK,EAAE,EAAE,UAAU,CAAC,CAAC;MAC7DW,aAAa,CAACC,OAAO,CAAEb,GAAG,IAAK;QAC/B,IAAIO,OAAM,GAAIP,GAAG,CAACK,IAAI,CAAC;QACvBE,OAAO,CAACO,EAAC,GAAId,GAAG,CAACc,EAAC;QAClB,IAAI,CAACR,QAAQ,CAACS,IAAI,CAACR,OAAO;QAC1BS,OAAO,CAACC,GAAG,CAACV,OAAO;MACrB,CAAC,CAAC;IACJ,CAAC;IACD,MAAMW,WAAWA,CAAA,EAAE;MACjB,MAAMC,MAAK,GAAI,MAAMrB,MAAM,CAACF,UAAU,CAACK,EAAE,EAAE,UAAU,CAAC,EAAE;QACtDO,MAAM,EAAE,IAAI,CAACD,OAAO,CAACC,MAAM;QAC3BC,MAAM,EAAE,IAAI,CAACF,OAAO,CAACE;MACvB,CAAC,EACEW,IAAI,CAAC,MAAM;QACVlB,MAAM,CAACmB,EAAE,CAAC,GAAG;QACbL,OAAO,CAACC,GAAG,CAAC,mBAAmB,CAAC;MAClC,CAAC,EACAK,KAAK,CAAC,UAASC,KAAK,EAAE;QACrBP,OAAO,CAACO,KAAK,CAAC,gCAAgC,EAAEA,KAAK,CAAC;MACxD,CAAC,CAAC;IACN,CAAC;IACD;IACA,MAAMC,YAAWA,CAAGV,EAAE,EAAC;MACrB,MAAMf,SAAS,CAACC,GAAG,CAACC,EAAE,EAAE,UAAU,EAAEa,EAAG,CAAC,CAAC;MACzCZ,MAAM,CAACmB,EAAE,CAAC,GAAG;IACf;EACF,CAAC;EACDI,OAAOA,CAAA,EAAG;IACR,IAAI,CAACd,YAAY,CAAC,CAAC;EACrB;AACF","ignoreList":[]}]}
CAAC,CAAC,CAAC,CAAC,CAAC;EACP,CAAC,CAAC,CAAC,CAAC,CAAC","file":"C:/Users/ratau/Desktop/ProyectoVueFirebase03/src/views/Home.vue","sourceRoot":"","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <Navbar/>\r\n      <!-- ////////// formulario Añadir ////////// -->\r\n    <!-- Nombre -->\r\n  <div class=\"container my-4\">\r\n  <form>  \r\n    <div class=\"input-group mb-3\">\r\n    <span class=\"input-group-text\">Nombre</span>\r\n    <input v-model=\"usuario.nombre\" type=\"text\" class=\"form-control\">\r\n    </div>\r\n    <!-- Correo -->\r\n    <div class=\"input-group mb-3\">\r\n    <span class=\"input-group-text\">Correo</span>\r\n    <input v-model=\"usuario.correo\" type=\"text\" class=\"form-control\">\r\n    </div>\r\n    <!-- Botone Guardar -->\r\n    <div class=\"mt-3\">  \r\n    <button @click.prevent=\"agregarDato()\" \r\n            class=\"btn btn-primary\">Guardar\r\n    </button>\r\n    </div>\r\n  </form>\r\n  </div>\r\n<!-- ////////// tabla ////////// -->\r\n  <table class=\"table\">\r\n    <thead>\r\n      <tr>\r\n        <th scope=\"col\">id</th>\r\n        <th scope=\"col\">Nombre</th>\r\n        <th scope=\"col\">Correo</th>\r\n        <th scope=\"col\">Eliminar</th>\r\n      </tr>\r\n    </thead>\r\n    <tbody>\r\n      <tr v-for=\"(item, index) in usuarios\" :key=\"index\">\r\n        <th scope=\"row\">{{index}}</th>\r\n        <td>{{item.nombre}}</td>\r\n        <td>{{item.correo}}</td> \r\n        <td>\r\n        <button @click.prevent=\"eliminarDato(item.id)\" \r\n          class=\"btn btn-danger\">Eliminar\r\n        </button>\r\n      </td>\r\n      </tr>\r\n    </tbody>\r\n  </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Navbar from '@/components/Navbar.vue'\r\nimport { collection, getDocs, addDoc, deleteDoc, doc } from 'firebase/firestore/lite';\r\nimport { db } from \"../main\";\r\nimport router from '../router/index'\r\n\r\nexport default {\r\n  name: 'Home',\r\n  components: {\r\n    Navbar\r\n  },\r\n  data() {\r\n    return {\r\n      usuarios: [],\r\n      usuario: {\r\n      nombre: '',\r\n      correo: ''\r\n    }\r\n    }\r\n  },\r\n  methods: {\r\n    async obtenerDatos () { \r\n      const querySnapshot = await getDocs(collection(db, \"usuarios\"));\r\n        querySnapshot.forEach((doc) => {\r\n        let usuario = doc.data()\r\n        usuario.id = doc.id\r\n        this.usuarios.push(usuario)\r\n        console.log(usuario)\r\n      });\r\n    },\r\n    async agregarDato(){\r\n      const docRef = await addDoc(collection(db, \"usuarios\"), {\r\n        nombre: this.usuario.nombre,\r\n        correo: this.usuario.correo\r\n      })\r\n        .then(() => {\r\n          router.go('/')\r\n          console.log(\"Documento añadido\");\r\n        })\r\n        .catch(function(error) {\r\n          console.error(\"Error al añadir el documento: \", error);\r\n        });\r\n    },\r\n    // DELETE / ELIMINAR / BORRAR\r\n    async eliminarDato (id){\r\n      await deleteDoc(doc(db, \"usuarios\", id ));\r\n      router.go('/')\r\n    },\r\n  },\r\n  mounted() {\r\n    this.obtenerDatos();\r\n  },\r\n}\r\n</script>\r\n"]}]}